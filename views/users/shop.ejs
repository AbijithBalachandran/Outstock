  <nav aria-label="Page navigation example" class="d-flex justify-content-end me-4">
    <ul class="pagination">
        <% if (currentPage > 1) { %>
            <li class="page-item">
                <a class="page-link" href="/shop?page=<%= currentPage - 1 %>&sort_by=<%= sortBy %>">Previous</a>
            </li>
        <% } else { %>
            <li class="page-item disabled">
                <a class="page-link" href="javascript:void(0);">Previous</a>
            </li>
        <% } %>

        <% for (let i = 1; i <= totalPages; i++) { %>
            <li class="page-item <%= i === currentPage ? 'active' : '' %>">
                <a class="page-link" href="/shop?page=<%= i %>&sort_by=<%= sortBy %>"><%= i %></a>
            </li>
        <% } %>

        <% if (currentPage < totalPages) { %>
            <li class="page-item">
                <a class="page-link" href="/shop?page=<%= currentPage + 1 %>&sort_by=<%= sortBy %>">Next</a>
            </li>
        <% } else { %>
            <li class="page-item disabled">
                <a class="page-link" href="javascript:void(0);">Next</a>
            </li>
        <% } %>
    </ul>
</nav>



